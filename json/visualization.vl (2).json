{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "COVID-19 Active Cases (10 Oct 2020)",
  "width": 800,
  "height": 600,
  "projection": {"type": "equirectangular"},
  "data": {
    "url": "https://raw.githubusercontent.com/mtoh0002/Visualisation-2/master/json/LGA_2016_AUST.topojson",
    "format": {"type": "topojson", "feature": "LGA_2016_AUST"}
  },
  "transform": [
    {
      "lookup": "properties.LGA_NAME16",
      "from": {
        "data": {
          "url": "https://github.com/mtoh0002/Visualisation-2/blob/master/data/fatalities per lga.csv"
        },
        "key": "LGA",
        "fields": ["Fatalities"],
        "type": "quantitative",
        "scale": {"domain": [0, 50]}
      }
    }
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "Fatalities",
      "type": "quantitative",
      "scale": {"domain": [0, 50]},
      "tooltip": [
        {"field": "properties.LGA_NAME16", "type": "nominal", "title": "LGA"},
        {"field": "Fatalities"}
      ]
    }
  },
  "config": {}
}